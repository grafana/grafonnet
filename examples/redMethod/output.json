{
   "description": "RED dashboard for Faro",
   "graphTooltip": 1,
   "panels": [
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 0
         },
         "id": 1,
         "panels": [ ],
         "title": "API",
         "type": "row"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 100,
                  "lineWidth": 0,
                  "showPoints": "never",
                  "stacking": {
                     "mode": "normal"
                  }
               },
               "min": 0,
               "unit": "reqps"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 1
         },
         "id": 2,
         "pluginVersion": "v10.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum by (status) (\n  label_replace(\n  label_replace(\n    rate(request_duration_seconds_count{cluster=\"$cluster\",namespace=\"$namespace\",job=~\".*/faro-api\"}[$__rate_interval])\n  ,\"status\", \"${1}xx\", \"status_code\", \"([0-9])..\")\n  ,\"status\", \"${1}\", \"status_code\", \"([a-z]+)\")\n)\n",
               "legendFormat": "{{status}}"
            }
         ],
         "title": "Requests / sec",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "min": 0,
               "unit": "ms"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 1
         },
         "id": 3,
         "pluginVersion": "v10.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "histogram_quantile(0.99,\n  sum by (le) (\n    rate(request_duration_seconds_bucket{cluster=\"$cluster\",namespace=\"$namespace\",job=~\".*/faro-api\"}[$__rate_interval])\n  )\n) * 1e3\n",
               "legendFormat": "99th Percentile"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "histogram_quantile(0.5,\n  sum by (le) (\n    rate(request_duration_seconds_bucket{cluster=\"$cluster\",namespace=\"$namespace\",job=~\".*/faro-api\"}[$__rate_interval])\n  )\n) * 1e3\n",
               "legendFormat": "50th Percentile"
            }
         ],
         "title": "Latency",
         "type": "timeseries"
      },
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 9
         },
         "id": 4,
         "panels": [ ],
         "title": "Collector",
         "type": "row"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 100,
                  "lineWidth": 0,
                  "showPoints": "never",
                  "stacking": {
                     "mode": "normal"
                  }
               },
               "min": 0,
               "unit": "reqps"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 10
         },
         "id": 5,
         "pluginVersion": "v10.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum by (status) (\n  label_replace(\n  label_replace(\n    rate(request_duration_seconds_count{cluster=\"$cluster\",namespace=\"$namespace\",job=~\".*/faro-collector\"}[$__rate_interval])\n  ,\"status\", \"${1}xx\", \"status_code\", \"([0-9])..\")\n  ,\"status\", \"${1}\", \"status_code\", \"([a-z]+)\")\n)\n",
               "legendFormat": "{{status}}"
            }
         ],
         "title": "Requests / sec",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "min": 0,
               "unit": "ms"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 10
         },
         "id": 6,
         "pluginVersion": "v10.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "histogram_quantile(0.99,\n  sum by (le) (\n    rate(request_duration_seconds_bucket{cluster=\"$cluster\",namespace=\"$namespace\",job=~\".*/faro-collector\"}[$__rate_interval])\n  )\n) * 1e3\n",
               "legendFormat": "99th Percentile"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "histogram_quantile(0.5,\n  sum by (le) (\n    rate(request_duration_seconds_bucket{cluster=\"$cluster\",namespace=\"$namespace\",job=~\".*/faro-collector\"}[$__rate_interval])\n  )\n) * 1e3\n",
               "legendFormat": "50th Percentile"
            }
         ],
         "title": "Latency",
         "type": "timeseries"
      }
   ],
   "schemaVersion": 36,
   "templating": {
      "list": [
         {
            "name": "datasource",
            "query": "prometheus",
            "regex": "(ops|dev)-cortex",
            "type": "datasource"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "name": "cluster",
            "query": "label_values(request_duration_seconds_count{job=~\".*/faro-.*\"}, cluster)",
            "type": "query"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "name": "namespace",
            "query": "label_values(request_duration_seconds_count{cluster=\"$cluster\", job=~\".*/faro-.*\"}, namespace)",
            "type": "query"
         }
      ]
   },
   "time": {
      "from": "now-6h",
      "to": "now"
   },
   "timezone": "utc",
   "title": "Faro RED dashboard",
   "uid": "faro-red-dashboard-demo"
}
